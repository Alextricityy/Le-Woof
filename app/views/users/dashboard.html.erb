<div class="container dashboard">
  <div class="row">
    <div class="col-xs-12 col-xs-offset-1">

      <h1>Dashboard</h1>
      <hr>

      <h2> My Bookings </h2>

      <div class="row heading">
       <div class="col-xs-2">
         <h3> Booked With </h3>
       </div>
       <div class="col-xs-2">
         <h3> Start Time </h3>
       </div>
       <div class="col-xs-2">
         <h3> End Time</h3>
       </div>
       <div class="col-xs-1">
         <h3> Price</h3>
       </div>
       <div class="col-xs-2">
         <h3> Status</h3>
       </div>
     </div>
     <% @bookings.each do |booking| %>
     <div class="row">
       <div class="col-xs-2">
        <%=link_to booking.dog do%>
        <p> <%=booking.dog.name.capitalize%> </p>
        <%end%>
      </div>
      <div class="col-xs-2">
       <p> <%=booking.start_time %></p>
     </div>
     <div class="col-xs-2">
       <p> <%=booking.end_time%></p>
     </div>
     <div class="col-xs-1">
       <p> Â£<%=booking.price%></p>
     </div>
     <div class="col-xs-2">
       <p> Pending</p>
     </div>
   </div>
   <%end%>

 <hr>
<h2> My Dogs' Bookings </h2>

      <div class="row heading">
       <div class="col-xs-1">
         <h3> Dog </h3>
       </div>
       <div class="col-xs-1">
         <h3> Borrower</h3>
       </div>
       <div class="col-xs-2">
         <h3> Start Time </h3>
       </div>
       <div class="col-xs-2">
         <h3> End Time</h3>
       </div>
       <div class="col-xs-1">
         <h3> Price</h3>
       </div>
       <div class="col-xs-2">
         <h3> Status</h3>
       </div>
     </div>
<% @user.dogs.each do |dog| %>
     <div class="row">
       <div class="col-xs-2">
        <%=link_to dog do%>
        <p> <%=dog.name.capitalize%> </p>
        <%end%>

      </div>
    </div>
     <%end%>







</div>
</div>
</div>
